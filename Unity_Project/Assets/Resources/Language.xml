<?xml version="1.0" encoding="iso-8859-1"?>
<Languages>
	<French text="francais">
		<Menu>
			<Setup_scene>			
				<HMD_Setup>
					<Title_Text text = "Casque non configuré" font-style="Normal" font-size="10" text-align="Center" />
					<Description_Text text = "Merci d'appuyer sur le bouton X pour commencer la configuration." font-style="Normal" font-size="10" text-align="Center" />
				</HMD_Setup>
				
				<UI_Interaction>
					<Title_Text text = "Commençons" font-style="Normal" font-size="10" text-align="Center" />
					<Description_Text text ="Regardez le bouton ci-dessous et laissez appuyé le bonton X" font-style="Normal" font-size="10" text-align="Center" />
					<Button_Text text ="Regardez ici" font-style="Normal" font-size="10" text-align="Center"/>
				</UI_Interaction>
				
				<Recenter>
					<Title_Text text ="Bien" font-style="Normal" font-size="10" text-align="Center" />
					<Description_Text text ="Le menu ne vous semble pas en face de vous ? Vous pouvez laisser appuyer le bouton Start afin de le replacer. Essayez maintenant." font-style="Normal" font-size="10" text-align="Center" />
				</Recenter>
				
				<Finished>
					<Title_Text text = "Configuration terminée" font-style="Normal" font-size="10" text-align="Center" />
					<Description_Text text = "Merci et bon ammusement." font-style="Normal" font-size="10" text-align="Center" />
					<Button_Text text = "Menu principal" font-style="Normal" font-size="10" text-align="Center" />
				</Finished>				
			</Setup_scene>
			
			<Intro>
				<Main_Menu>
					<Button_Start text = "Commencer" font-style="Normal" font-size="10" text-align="Center" />	
					<Button_Options text = "Options" font-style="Normal" font-size="10" text-align="Center" />
          <Button_Credits text = "Credits" font-style="Normal" font-size="10" text-align="Center" />
        </Main_Menu>
			</Intro>
		</Menu>
		
		<Gameplay>
      <HUD>
        <InfoCockpit text = "! COCKPIT VERROUILLER !" font-style="Normal" font-size="10" text-align="Center" />
        <InfoLeftWeapons text = "Chaleur arme gauche" font-style="Normal" font-size="10" text-align="Center" />
        <InfoRightWeapons text = "Chaleur arme droite" font-style="Normal" font-size="10" text-align="Center" />
      </HUD>
      <Pause_Menu>
        <Button_Resume text = "Retour" font-style="Normal" font-size="10" text-align="Center" />
        <Button_Return text = "Revenir au menu" font-style="Normal" font-size="10" text-align="Center" />
      </Pause_Menu>
		</Gameplay>
	</French>
	
	<English text="English">
		<Menu>
			<Setup_scene>			
				<HMD_Setup>
					<Title_Text text = "HMD not setup" font-style="Normal" font-size="10" text-align="Center" />
					<Description_Text text = "Please PRESS X button to begin setup." font-style="Normal" font-size="10" text-align="Center" />
				</HMD_Setup>
				
				<UI_Interaction>
					<Title_Text text = "Let's Begin" font-style="Normal" font-size="10" text-align="Center" />
					<Description_Text text ="LOOK AT INTERACTABLES, HOLD X TO SELECT. Let’s practice! Select the button below" font-style="Normal" font-size="10" text-align="Center" />
					<Button_Text text ="Look here!" font-style="Normal" font-size="10" text-align="Center"/>
				</UI_Interaction>
				
				<Recenter>
					<Title_Text text ="Great!" font-style="Normal" font-size="10" text-align="Center" />
					<Description_Text text ="FEELING LOST? HOLD OPTIONS OR START TO RESET YOUR POSITION AND ROTATION. Try this now!" font-style="Normal" font-size="10" text-align="Center" />
				</Recenter>
				
				<Finished>
					<Title_Text text = "Setup complete" font-style="Normal" font-size="10" text-align="Center" />
					<Description_Text text = "Thanks, have fun" font-style="Normal" font-size="10" text-align="Center" />
					<Button_Text text = "Main menu" font-style="Normal" font-size="10" text-align="Center" />
				</Finished>				
			</Setup_scene>
			
			<Intro>
				<Main_Menu>
					<Button_Start text = "Start" font-style="Normal" font-size="10" text-align="Center" />	
					<Button_Options text = "Options" font-style="Normal" font-size="10" text-align="Center" />
          <Button_Credits text = "Credits" font-style="Normal" font-size="10" text-align="Center" />
        </Main_Menu>	
			</Intro>
		</Menu>
		
		<Gameplay>
      <HUD>
        <InfoCockpit text = "! MECHA LOCKED ON !" font-style="Normal" font-size="10" text-align="Center" />
        <InfoLeftWeapons text = "left weapon heat" font-style="Normal" font-size="10" text-align="Center" />
        <InfoRightWeapons text = "right weapon heat" font-style="Normal" font-size="10" text-align="Center" />
      </HUD>
      <Pause_Menu>
        <Button_Resume text = "Resume" font-style="Normal" font-size="10" text-align="Center" />
        <Button_Return text = "Back to main menu" font-style="Normal" font-size="10" text-align="Center" />
      </Pause_Menu>
		</Gameplay>
	</English>
	
